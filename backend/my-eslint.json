{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "eqeqeq": "off",
        "curly": "error",
        "quotes": ["error", "double"],
        "no-compare-neg-zero": "error",
        "no-dupe-args": "error",
        "no-dupe-keys": "error",
        "no-empty": "error",
        "no-empty-character-class": "error",
        "no-extra-parens": "error",
        "no-extra-semi": "error",
        "no-import-assign": "off",
        "no-misleading-character-class": "error",
        "no-obj-calls": "error",

        "no-unused-vars": 0
    }
}
